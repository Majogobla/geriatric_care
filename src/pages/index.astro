---
import Layout from '../layouts/Layout.astro'
import Header from '../sections/Header.astro'
import PostHeader from '../sections/PostHeader.astro'
import Message from '../components/Message.astro'
import Services from '../components/Services.astro'
import Prices from '../components/Prices.astro'
import Footer from '../components/Footer.astro'
import ContactUs from '../components/ContactUs.astro'
import NetworkButtons from '../components/NetworkButtons.astro'
import Gallery from '../components/Gallery.jsx'
import type { onPageLoad } from 'astro/virtual-modules/transitions-events.js'
---

<Layout title="Geriatric Care">
	<div class=" min-h-screen dark:bg-[#353535]">
		<Header/>

		<PostHeader/>

		<Message/>

		<Services/>

		<Prices/>

		<Gallery client:only="react"/>

		<ContactUs/>
		
		<NetworkButtons/>

		<Footer/>
	</div>
</Layout>
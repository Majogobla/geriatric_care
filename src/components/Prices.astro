<script>
  const firstPrice = document.getElementById("first-price")
  const secondPrice = document.getElementById("second-price")
  const thirdPrice = document.getElementById("third-price")
  
  delayCard(window.innerWidth)
  
  window.addEventListener('resize', () => {
    delayCard(window.innerWidth)
  })
  
  function delayCard(size) {
    if(size < 1024) {
      firstPrice.removeAttribute('data-aos-offset')
      secondPrice.removeAttribute('data-aos-offset')
      thirdPrice.removeAttribute('data-aos-offset')
    } else {
      firstPrice.setAttribute('data-aos-offset', '100')
      secondPrice.setAttribute('data-aos-offset', '300')
      thirdPrice.setAttribute('data-aos-offset', '600')
    }
  }
</script>